{
  "default": true,
  "line-length": {
    "line_length": 100,
    "code_blocks": false,
    "tables": false,
    "headings": false
  },
  "no-duplicate-heading": {
    "siblings_only": true
  },
  "no-trailing-punctuation": {
    "punctuation": ".,;:"
  },
  "ul-style": {
    "style": "dash"
  },
  "ol-prefix": {
    "style": "ordered"
  },
  "no-inline-html": false,
  "first-line-heading": false,
  "fenced-code-language": false,
  "blanks-around-fences": true,
  "blanks-around-lists": true,
  "proper-names": {
    "names": [
      "JavaScript",
      "TypeScript",
      "React",
      "FastAPI",
      "OpenAI",
      "Python",
      "Node.js",
      "Redis",
      "Celery",
      "Render.com",
      "GitHub"
    ]
  }
}