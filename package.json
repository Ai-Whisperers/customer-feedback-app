{
  "name": "customer-feedback-analyzer",
  "version": "3.2.0",
  "private": true,
  "description": "Customer AI Driven Feedback Analyzer - Monorepo",
  "workspaces": [
    "web"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=web",
    "build": "npm run build --workspace=web",
    "start": "npm run start --workspace=web",
    "install:all": "npm install && npm install --workspace=web",
    "deploy:check": "cd web && npm run type-check && npm run lint",
    "clean": "rm -rf web/dist web/node_modules api/__pycache__",
    "fresh": "npm run clean && npm run install:all"
  },
  "devDependencies": {
    "@types/node": "^22.8.1",
    "typescript": "^5.6.3",
    "tsx": "^4.7.0",
    "vite": "^5.4.10",
    "@vitejs/plugin-react": "^4.3.3"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.1",
    "express": "^4.18.2",
    "compression": "^1.7.4",
    "helmet": "^8.0.0",
    "dotenv": "^16.3.1",
    "http-proxy-middleware": "^3.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}