{
  "name": "web",
  "version": "3.2.0",
  "private": true,
  "description": "Customer Feedback Analyzer - Frontend & BFF",
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:vite": "vite",
    "dev:bff": "tsx watch bff/server.ts",
    "build": "npm run build:client && npm run build:bff",
    "build:client": "tsc -b && vite build",
    "build:bff": "tsc -p tsconfig.bff.json",
    "start": "NODE_ENV=production node dist/bff/server.js",
    "preview": "vite preview",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "plotly.js-dist-min": "^2.35.0",
    "react-plotly.js": "^2.6.0",
    "react-dropzone": "^14.2.9",
    "zod": "^4.1.9",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-plotly.js": "^2.6.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@eslint/js": "^9.13.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "terser": "^5.44.0"
  }
}