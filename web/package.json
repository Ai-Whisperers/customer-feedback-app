{
  "name": "customer-feedback-analyzer-web",
  "version": "3.1.0",
  "private": true,
  "description": "Customer AI Driven Feedback Analyzer - Frontend & BFF",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsc server/server.ts --outDir dist/server",
    "preview": "vite preview",
    "server": "tsx server/server.ts",
    "start": "node dist/server/server.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "plotly.js": "^2.35.2",
    "react-plotly.js": "^2.6.0",
    "axios": "^1.7.7",
    "react-dropzone": "^14.2.9",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "express": "^4.21.1",
    "http-proxy-middleware": "^3.0.3",
    "compression": "^1.7.4",
    "helmet": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/node": "^22.8.1",
    "@types/express": "^5.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "tsx": "^4.19.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/user-event": "^14.5.2",
    "jsdom": "^25.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
