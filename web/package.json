{
  "name": "customer-feedback-analyzer-web",
  "version": "3.1.0",
  "private": true,
  "description": "Customer AI Driven Feedback Analyzer - Frontend & BFF",
  "scripts": {
    "dev": "tsx server/server.ts",
    "dev:client": "cd client && npm run dev",
    "build": "cd client && npm install && npm run build && cd .. && tsc server/server.ts --outDir dist --esModuleInterop --module commonjs --target es2020 && cp -r client/dist dist/client-build",
    "build:render": "npm ci --production=false && cd client && npm ci --production=false && npm run build && cd .. && npx tsc server/server.ts --outDir dist --esModuleInterop --module commonjs --target es2020 && (cp -r client/dist dist/client-build 2>/dev/null || xcopy /E /I /Y client\\dist dist\\client-build)",
    "start:prod": "node dist/server.js",
    "server": "tsx server/server.ts",
    "lint": "cd client && npm run lint",
    "type-check": "cd client && tsc --noEmit"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "http-proxy-middleware": "^3.0.3",
    "plotly.js": "^2.35.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.9",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/helmet": "^0.0.48",
    "@types/node": "^22.18.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/ui": "^2.1.4",
    "autoprefixer": "^10.4.20",
    "axios": "^1.12.2",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "form-data": "^4.0.4",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
